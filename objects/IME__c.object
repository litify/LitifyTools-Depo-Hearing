        <description>generates a printable text value of the ime time</description>
        <externalId>false</externalId>
        <formula>IF (ISBLANK( Time_of_IME__c), &quot;&quot;,
TEXT(IF(HOUR(Time_of_IME__c) &lt;= 12, HOUR(Time_of_IME__c) , (HOUR(Time_of_IME__c) -12) ))
&amp; &quot;:&quot; &amp; LPAD(TEXT(MINUTE(Time_of_IME__c)),2,&apos;0&apos;) &amp; IF (HOUR(Time_of_IME__c) &lt; 12, &quot; AM&quot;, &quot; PM&quot;)
)</formula>
